<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Preference
        app:key="account_options"
        app:title="@string/account" />

    <ListPreference
        app:title="Languages"
        app:key="language_pref"
        app:entries="@array/language_string"
        app:entryValues="@array/language_values"/>

    <Preference
        app:key="notification_settings"
        app:title="Notification Settings" />

</PreferenceScreen>